{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Run Data Processor",
            "type": "shell",
            "command": "dart",
            "args": [
                "run",
                "bin/sensor_simulator.dart"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "isBackground": true,
           "problemMatcher": {
                "owner": "dart",
                "fileLocation": ["relative", "${workspaceFolder}"],
                "pattern": {
                    "regexp": "",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                },
                "background": {
                    "activeOnStart": true,
                    "beginsPattern": "^.*$",
                    "endsPattern": "^$"
                }
            }
        }
    ]
}